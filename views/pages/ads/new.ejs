                                            <img src="https://carurac.com/newimage/small/2003-206-facelift-2003-peugeot.jpg"
                                                class="pb-1 rounded-lg" alt="..." />
                                            <img src="data:image/jpeg;base64,"
                                                class="pb-1 rounded-lg" alt="..." />
                                        </div>
                                    </div>
                                    <div class="dropzone mb-2 d-flex justify-content-center bg-accent-15 rounded-lg p-5"
                                        style="border: 1px rgba(0, 0, 0, .25) dashed;">

                                        <label class="position-relative btn btn-cta btn-accent px-3 px-sm-4">
                                            <input id="mediaInput" onchange="preview(event)"
                                                style="cursor: pointer; top: 0; left: 0"
                                                class="position-absolute opacity-0 w-100 h-100" type="file" multiple>
                                            <script>
                                                function preview(event) {
                                                    if (event.target.files.length > 0) {
                                                        let mediaElement = document.querySelector('.media-preview .media');
                                                        let filesSize = 0;
                                                        mediaElement.innerHTML = '';
                                                        console.log(event.target.files);
                                                        for (let file of event.target.files) {
                                                            let reader = new FileReader();
                                                            reader.onload = () => {
                                                                let img = document.createElement('img');
                                                                img.src = reader.result;
                                                                img.setAttribute('class', 'rounded-lg pb-1 pe-1')
                                                                mediaElement.appendChild(img);
                                                                filesSize += file.size
                                                                console.log(filesSize)
                                                            }
                                                            reader.onloadend = () => {
                                                                function bytesToSize(bytes) {
                                                                    var sizes = ['Bytes', 'KB', 'MB', 'GB', 'TB'];
                                                                    if (bytes == 0) return '0 Byte';
                                                                    var i = parseInt(Math.floor(Math.log(bytes) / Math.log(1024)));
                                                                    return Math.round(bytes / Math.pow(1024, i), 2) + ' ' + sizes[i];
                                                                }
                                                                document.querySelector('.media-preview h6').innerHTML = `${event.target.files.length} Photo(s) (${bytesToSize(filesSize)})`
                                                            }
                                                            reader.readAsDataURL(file)
                                                        }
                                                    }
                                                }
                                            </script>
